dict={'radhika':45,'vaishnavi':66,'divya':75,'varsha':78,'srivalli':20,'satvika':85,'srinija':89,'keerthi':30,'deepika':40,'kailash':100}
dict['rahul']=84
dict['srivali']=50
for i in dict:
    print(i,dict[i])
sum=0    
for i in dict:
    sum=sum+dict[i]
average=sum/len(dict)
print(average)
    
  
